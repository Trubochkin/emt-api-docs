### Basic JSON data format for all device
{
    "ID": "xxxxxxxxxxxxxxxx",
	"HW_VERSION": "x.x",
    "FW_VERSION": "x.x.x",
	"TYPE": "type_name",
	"NAME": "name",
	"STATUS": null,
	"SETTINGS": null,
    "DATA": [
		{
			"UUID": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
			"LABEL": "lable_name",
			"DATAPOINTS": [
				[UNIX_time_ms, value],
				[1566984660000, 25],
				[1566984720000, 39],
				[1566984780000, 56],
				[1566984840000, 41],
				[1566984900000, 20],
			]
		}
	]
}